                <span class="brand-text font-weight-light">Cari Paneli</span>
            </a>

            <!-- Sidebar -->
            <!-- Sidebar -->
            <div class="sidebar">

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
